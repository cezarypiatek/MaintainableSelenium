	  <dependency id="Tellurium.MvcPages" version="$version$" />
	  <dependency id="TeamCity.ServiceMessages" version="3.0.5.1" />
    </dependencies>	
  </metadata>
  <files>
		<file src="bin\$configuration$\Tellurium.VisualAssertions.dll" target="lib" />
		<file src="bin\$configuration$\Tellurium.VisualAssertions.pdb" target="lib" />
